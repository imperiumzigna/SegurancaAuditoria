@{
    ViewBag.Title = User.Identity.Name;
}


<div class="telaBatePapo">

    <div class="retangulo">
        <div class="retangulo1">
            <h2 class="center-align">Conversas</h2>

        </div>
        <div class="retangulo2">
            <div class="conversaHeader">
                <p>Fulano</p>
            </div>

            <div id="conversas">
                
            </div>

            <div class="janelaConteudo">

                <form>
                    <button id="enviar_btn" class="btn btn-enviar waves-effect waves-light" type="submit" name="action">
                        Enviar
                        <i class="material-icons right ">send</i>
                    </button>

                    <input id="conteudo" type="text" class="validate">
                </form>
            </div>
        </div>
    </div>
    
</div>


@section ChatJs{
    @Scripts.Render("~/bundles/chat")
@Scripts.Render("~/bundles/jquery")
}

